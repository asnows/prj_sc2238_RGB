#ifndef SRC_DDR_ALLOC_H_
#define SRC_DDR_ALLOC_H_

#define DDR_BASE_ADDR 				0x00100000
#define DDR_RX_BD_START_ADDR 		0x0ff00000  // Receive Buffer Descriptor List start address
#define DDR_RX_BUF_START_ADDR		0x0ff00100  // eth0 receive buffer start address


#endif /* SRC_DDR_ALLOC_H_ */
